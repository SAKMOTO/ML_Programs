'''import pandas as pd 
import warnings
from sklearn.tree import DecisionTreeRegressor
import matplotlib.pyplot as plt 
from sklearn.tree import plot_tree

df = pd.DataFrame({
    'housesize': [1500, 1800, 1200, 2100, 3000],
    'price': [200000, 250000, 180000, 270000, 400000]  # Fixed the prices (removed extra zeros)
})

X = df[['housesize']]
y = df['price']
model = DecisionTreeRegressor(max_depth=2)
model.fit(X, y)

